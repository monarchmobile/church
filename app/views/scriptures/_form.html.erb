<%= form_for @scripture do |f| %> 
	<%= render 'shared/errors', object: @scripture %> 
	<div class="field">
		<%= f.label :title %>
		<%= f.text_field :title %>
	</div>

	<div class="field">
		<%= f.label :content %>
		<%= f.text_area :content, :cols => 100, :rows => 10 %>
	</div>
	
	<div class="actions">
		<%= f.submit %>
	</div>


<% end %>